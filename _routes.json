{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/_headers", "/_redirects", "/build-postprocess.js"],
  "routes": [
    {
      "src": "/assets/.*\\.css",
      "headers": {
        "Content-Type": "text/css",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/assets/.*\\.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}